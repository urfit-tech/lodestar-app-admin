{
  "name": "lodestar-app-admin-xuemi",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/charts": "^1.0.20",
    "chance": "^1.1.7",
    "lodestar-app-admin": "git+https://5283f1eb2c0cbdec821bd2bc65e18c942c4d51f8:x-oauth-basic@github.com/urfit-tech/lodestar-app-admin.git#master"
  },
  "scripts": {
    "postinstall": "yarn upgrade lodestar-app-admin",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "CI=false react-app-rewired build",
    "build:staging": "env-cmd -f .env.development npm run build",
    "test": "react-scripts test",
    "typegen": "apollo client:codegen ./src/hasura.d.ts --outputFlat --target typescript",
    "format:organize-imports": "organize-imports-cli ./src/**/*.ts[x]",
    "format:prettier": "prettier --write ./src/**/*.ts[x]",
    "format": "npm run format:organize-imports && npm run format:prettier",
    "trans:compile": "tsc --noEmit false --outDir src/extracted",
    "trans:extract": "babel --config-file ./.intl.babelrc ./src/extracted > /dev/null",
    "trans:manage": "node manageTranslations.js",
    "trans:clean": "rm -rf ./src/extracted",
    "trans": "npm run trans:clean && npm run trans:compile && npm run trans:extract && npm run trans:manage && npm run trans:clean"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "organize-imports-cli",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/audiobuffer-to-wav": "^1.0.0",
    "@types/braft-editor": "^2.2.0",
    "@types/chance": "^1.1.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.35",
    "@types/ramda": "^0.27.39",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-star-rating-component": "^1.4.0",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@types/wavesurfer.js": "^3.3.1",
    "apollo": "^2.32.5",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-react-intl": "^8.2.7",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lint-staged": "^10.5.4",
    "organize-imports-cli": "^0.8.0",
    "prettier": "^2.2.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.8",
    "react-hot-loader": "^4.13.0",
    "react-intl-translations-manager": "^5.0.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.2.3"
  }
}
